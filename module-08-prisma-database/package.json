{
  "name": "module-08-prisma-database",
  "version": "1.0.0",
  "description": "GraphQL Yoga with Prisma ORM and SQLite database - demonstrating context-based database injection",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset && npm run db:seed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "graphql": "^16.12.0",
    "graphql-yoga": "^5.18.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "prisma": "^6.9.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
